<h2>Posts</h2>

<table class="table">
  <thead>
    <tr>
      <th>
        id
      </th>
      <th>
        Date
      </th>
      <th>
        User
      </th>
      <th>
        rationale
      </th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td>
          <%= post.id %>
        </td>
        <td>
          <%= post.date %>
        </td>
        <td>
          <%= post.user.last_name %>
        </td>
        <td>
          <%= post.rationale %>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>








